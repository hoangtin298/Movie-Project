#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@material-ui/x-grid-data-generator/bin/data-gen-script.js" "$@"
  ret=$?
else 
  node  "$basedir/../@material-ui/x-grid-data-generator/bin/data-gen-script.js" "$@"
  ret=$?
fi
exit $ret
